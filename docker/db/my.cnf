[mysqld]
datadir=/var/lib/mysql
default-storage-engine=InnoDB
character-set-server=utf8
#general_log=1
#general_log_file=/var/log/mysql/query.log
slow_query_log=1
slow_query_log_file=/var/log/mysql/slow_query.log
long_query_time=3

[client]
default-character-set = utf8

[mysqldump]
default-character-set = utf8
